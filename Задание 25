#include <iostream>
#include <math.h>
 
using namespace std;
 
int main() {
     
    double skr, skv;
     
    cout << "\n Поместится ли квадрат в круг?\n";
    cout << "-------------------------------\n";
     
    cout << " Введите площадь круга: ";
    cin >> skr;
     
    cout << " Введите площадь квадрата: ";
    cin >> skv;
     
    string s = skr < skv * M_PI / 2 ? "не " : "";
     
    cout << "-------------------------------\n";
    printf(" Квадрат %sпоместится в круг\n\n", s.data());
 
    return 0;
    // Таибов Баград Имамудинович. НПИбд-03-23.
    // Известны площади круга и квадрата. Определить, уместится ли квадрат в круге 
}
