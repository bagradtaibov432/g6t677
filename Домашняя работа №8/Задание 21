#include <iostream>


int main() {
    
    int number;
    
    std::cin >> number;
    
    while (number > 0) {
        int digit = number % 10;
        std::cout << digit;
        number /= 10;
    }
    
    std::cout << std::endl;

    return 0;
}
// Таибов Баград НПИбд-03-23
// Дано целое положительное число. Вывести символы,
// изображающие цифры этого числа (в порядке справа налево).
