#include <iostream>
#include <string>

int main() {
    int N;
    char C1, C2;
    
    std::cin >> N >> C1 >> C2;
    
    if (N % 2 == 0) {
        for (int i = 0; i < N; ++i) {
            std::cout <<(i % 2 == 0 ? C1 : C2);
            
        }
        std::cout << std::endl;
    }
    return 0;
}
