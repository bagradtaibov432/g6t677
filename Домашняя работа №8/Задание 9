#include <iostream>
#include <string>

int main() {
    int N;
    char C1, C2;
    
    std::cin >> N >> C1 >> C2;
    
    if (N % 2 == 0) {
        for (int i = 0; i < N; ++i) {
            std::cout <<(i % 2 == 0 ? C1 : C2);
            
        }
        std::cout << std::endl;
    }
    return 0;
}
// Таибов Баград НПИбд-03-23
// Дано четное число N (> 0) и символы C1 и C2. Вывести строку длины N, которая состоит
// из чередующихся символов C1 и C2, начиная с C1. В тестах к данной задаче кириллицы нет.
// Если N нечетное, то ничего не выводится.
